<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="35.0" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="3.0">
         <children>
            <Label fx:id="name" alignment="CENTER" text="Name" wrapText="true">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label prefWidth="175" fx:id="username" text="Username" />
         </children>
      </VBox>
      <ImageView fx:id="icon" fitHeight="36.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="-2.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
   </children>
</fx:root>
